{
  "name": "structured-clone-polyfill",
  "version": "0.0.2",
  "description": "from core-js polyfill",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/wwog/sClone",
  "bugs": "https://github.com/wwog/lib-template/issues",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "packageManager": "pnpm@7.5.1",
  "funding": "https://github.com/wwog",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsup",
    "lint": "eslint --cache --ext .tsx,.ts,.js,.jsx .",
    "lint:fix": "eslint --cache --ext .tsx,.ts,.js,.jsx . --fix",
    "format": "prettier --ignore-path .eslintignore --write ./src/**/*.ts"
  },
  "keywords": [],
  "author": "wwog",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "latest",
    "tsup": "^6.1.3"
  },
  "dependencies": {
    "core-js-pure": "^3.23.5"
  }
}